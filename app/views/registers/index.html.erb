<p id="notice"><%= notice %></p>

<h1>Listing Registers</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Year</th>
      <th>Colegio precedencia</th>
      <th>Pago</th>
      <th>Repitente</th>
      <th>Extracurricular</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registers.each do |register| %>
      <tr>
        <td><%= register.student_id %></td>
        <td><%= register.year %></td>
        <td><%= register.colegio_precedencia %></td>
        <td><%= register.pago %></td>
        <td><%= register.repitente %></td>
        <td><%= register.extracurricular %></td>
        <td><%= link_to 'Show', register %></td>
        <td><%= link_to 'Edit', edit_register_path(register) %></td>
        <td><%= link_to 'Destroy', register, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Register', new_register_path %>
